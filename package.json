{
  "name": "bank-analysis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:serve": "yarn build && serve -s build\n",
    "analyze": "yarn run build && source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "jest": "jest --passWithNoTests",
    "eject": "react-scripts eject",
    "tauri": "tauri",
    "dev": "yarn run tauri dev",
    "bundle": "yarn run tauri build",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,ts}\"",
    "lint": "eslint --cache \"**/*.{js,jsx,ts,tsx}\"",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@agile-ts/core": "^0.2.7",
    "@agile-ts/event": "^0.0.14",
    "@agile-ts/react": "^0.2.3",
    "@agile-ts/utils": "^0.0.11",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@react-spring/web": "^9.2.6",
    "@tauri-apps/api": "^1.0.0-beta.8",
    "chart.js": "^3.5.1",
    "create-styles": "^0.0.4",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.5",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.0.3",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@agile-ts/logger": "^0.0.11",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@tauri-apps/cli": "^1.0.0-beta.10",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.3",
    "@types/react-router-dom": "^5.3.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
